/* montserrat-regular - latin_cyrillic */
@font-face {
    font-family: 'Montserrat';
    font-style: normal;
    font-weight: 400;
    src: url('<%= asset_path('adm/montserrat-v15-latin_cyrillic-regular.eot') %>'); /* IE9 Compat Modes */
    src: local(''),
    url('<%= asset_path('adm/montserrat-v15-latin_cyrillic-regular.eot?#iefix') %>') format('embedded-opentype'), /* IE6-IE8 */
    url('<%= asset_path('adm/montserrat-v15-latin_cyrillic-regular.woff2') %>') format('woff2'), /* Super Modern Browsers */
    url('<%= asset_path('adm/montserrat-v15-latin_cyrillic-regular.woff') %>') format('woff'), /* Modern Browsers */
    url('<%= asset_path('adm/montserrat-v15-latin_cyrillic-regular.ttf') %>') format('truetype'), /* Safari, Android, iOS */
    url('<%= asset_path('adm/montserrat-v15-latin_cyrillic-regular.svg#Montserrat') %>') format('svg'); /* Legacy iOS */
}

