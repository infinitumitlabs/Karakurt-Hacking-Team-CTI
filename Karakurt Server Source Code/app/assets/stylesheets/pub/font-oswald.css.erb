/* oswald-200 - latin_cyrillic */
@font-face {
    font-family: 'Oswald';
    font-style: normal;
    font-weight: 200;
    font-display: swap;
    src: url('<%= asset_path('pub/oswald-v36-latin_cyrillic-200.eot') %>'); /* IE9 Compat Modes */
    src: local(''),
    url('<%= asset_path('pub/oswald-v36-latin_cyrillic-200.eot?#iefix') %>') format('embedded-opentype'), /* IE6-IE8 */
    url('<%= asset_path('pub/oswald-v36-latin_cyrillic-200.woff2') %>') format('woff2'), /* Super Modern Browsers */
    url('<%= asset_path('pub/oswald-v36-latin_cyrillic-200.woff') %>') format('woff'), /* Modern Browsers */
    url('<%= asset_path('pub/oswald-v36-latin_cyrillic-200.ttf') %>') format('truetype'), /* Safari, Android, iOS */
    url('<%= asset_path('pub/oswald-v36-latin_cyrillic-200.svg#Oswald') %>') format('svg'); /* Legacy iOS */
}

